<h1>Student Form</h1>
<%= form_tag students_path do %>
<%= text_field_tag :first_name, :'student[first_name]' %>
<%= text_area_tag :last_name, :'student[last_name]' %>
<%= submit_tag "Submit Student" %>
<% end %>
